<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="535.0" prefWidth="611.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.YouTubeSearchController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints valignment="CENTER" />
   </rowConstraints>
   <children>
      <JFXTextField fx:id="txt_search" maxWidth="420.0" minWidth="420.0" prefHeight="31.0" prefWidth="420.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <JFXButton fx:id="btn_search" onAction="#press_btn_search" style="-fx-background-color: #000000;" text="Search" textFill="WHITE" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
      <Label alignment="CENTER" contentDisplay="CENTER" text="Search Youtube" GridPane.halignment="CENTER">
         <font>
            <Font name="Arial Narrow" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tbl_results" onMouseClicked="#getLinkFromList" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="3">
        <columns>
          <TableColumn fx:id="col_titles" prefWidth="419.0" text="Video Titles" />
        </columns>
      </TableView>
      <JFXButton fx:id="btn_back" onAction="#press_btn_back" style="-fx-background-color: #c5c5c5;" text="Back" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
      <JFXButton fx:id="btn_addToDB" onAction="#press_btn_addtoDB" text="Add to DB" GridPane.rowIndex="4" />
   </children>
</GridPane>
